<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>SimpleJspServletDB</display-name>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <description/>
        <display-name>PreferencesController</display-name>
        <servlet-name>PreferencesController</servlet-name>
        <servlet-class>controller.PreferencesController</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>ScheduleController</display-name>
        <servlet-name>ScheduleController</servlet-name>
        <servlet-class>controller.ScheduleController</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>UnscheduledCoursesController</display-name>
        <servlet-name>UnscheduledCoursesController</servlet-name>
        <servlet-class>controller.UnscheduledCoursesController</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>SearchController</display-name>
        <servlet-name>SearchController</servlet-name>
        <servlet-class>controller.SearchController</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>RoomsController</display-name>
        <servlet-name>RoomsController</servlet-name>
        <servlet-class>controller.RoomsController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PreferencesController</servlet-name>
        <url-pattern>/PreferencesController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ScheduleController</servlet-name>
        <url-pattern>/ScheduleController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnscheduledCoursesController</servlet-name>
        <url-pattern>/UnscheduledCoursesController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchController</servlet-name>
        <url-pattern>/SearchController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoomsController</servlet-name>
        <url-pattern>/RoomsController</url-pattern>
    </servlet-mapping>
</web-app>
